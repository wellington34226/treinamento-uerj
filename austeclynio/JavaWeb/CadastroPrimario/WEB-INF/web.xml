<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
    
    <servlet>
      <servlet-name>cadastroInicial</servlet-name>
      <servlet-class>web.ControllerCadastrarDados</servlet-class>
    </servlet>	
    <servlet-mapping>
        <servlet-name>cadastroInicial</servlet-name>
        <url-pattern>/CadastroInicial</url-pattern>
    </servlet-mapping>
	
	<servlet>
      <servlet-name>retornaNome</servlet-name>
      <servlet-class>web.ControllerRetornaNome</servlet-class>
    </servlet>	
    <servlet-mapping>
        <servlet-name>retornaNome</servlet-name>
        <url-pattern>/RetornaNome</url-pattern>
    </servlet-mapping>
	
	<servlet>
      <servlet-name>cadastroInicialToken</servlet-name>
      <servlet-class>web.ControllerCadastrarDadosToken</servlet-class>
    </servlet>	
    <servlet-mapping>
        <servlet-name>cadastroInicialToken</servlet-name>
        <url-pattern>/CadastroInicialToken</url-pattern>
    </servlet-mapping>
	
	<servlet>
      <servlet-name>retornaNomeToken</servlet-name>
      <servlet-class>web.ControllerRetornaNomeToken</servlet-class>
    </servlet>	
    <servlet-mapping>
        <servlet-name>retornaNomeToken</servlet-name>
        <url-pattern>/RetornaNomeToken</url-pattern>
    </servlet-mapping>
	
	
  
  
</web-app>